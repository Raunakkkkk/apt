{
  "name": "realtime-orders-system",
  "version": "1.0.0",
  "description": "Real-time order tracking system using Node.js, PostgreSQL, and WebSockets",
  "main": "server.js",
  "scripts": {
    "backend": "node server.js",
    "frontend": "webpack serve --mode development",
    "backend:dev": "nodemon server.js",
    "build": "webpack --mode production",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"No linter configured\"",
    "setup": "echo \"Run: psql -U orders_user -d orders_db -f init.sql\""
  },
  "keywords": [
    "nodejs",
    "postgresql",
    "websocket",
    "realtime",
    "orders",
    "notify",
    "listen"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.11.3",
    "ws": "^8.14.2",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
